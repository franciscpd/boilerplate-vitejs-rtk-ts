{
  "name": "boilerplate-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "watch-codegen": "graphql-codegen --config codegen.yml --watch 'src/**/*.graphql'",
    "prepare": "husky install",
    "lint": "eslint --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "npx prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.5",
    "@reduxjs/toolkit": "^1.8.3",
    "@rtk-query/graphql-request-base-query": "^2.2.0",
    "classnames": "^2.3.1",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "react": "^18.2.0",
    "react-daisyui": "^2.1.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "6",
    "react-select": "^5.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@graphql-codegen/add": "^3.2.0",
    "@graphql-codegen/cli": "^2.8.1",
    "@graphql-codegen/near-operation-file-preset": "^2.3.1",
    "@graphql-codegen/typed-document-node": "^2.3.1",
    "@graphql-codegen/typescript": "^2.7.1",
    "@graphql-codegen/typescript-operations": "^2.5.1",
    "@graphql-codegen/typescript-rtk-query": "^2.3.1",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-env": "^1.17.0",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitest/ui": "^0.18.1",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "daisyui": "^2.19.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.1",
    "vitest": "^0.18.1"
  }
}
